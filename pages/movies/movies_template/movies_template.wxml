<import src="../stars/stars_template.wxml" />
<template name="moviesTemplate">
    <view class="movies_container" catchtap="onMovieTap" data-movieId="{{movieId}}">
        <image class="movies_image" src="{{coverageUrl}}"></image>
        <text class="movies_title">{{title}}</text>
        <template is="starsTemplate" data="{{stars:stars,score:average}}"/>
    </view>
</template>
